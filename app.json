{
  "expo": {
    "name": "Background Changer",
    "slug": "BackgroundChanger",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "Just changes color dynamically"
  }
}
